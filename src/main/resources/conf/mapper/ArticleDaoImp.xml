<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.safecode.dao.ArticleDao">
<select id="query" resultType="com.safecode.entity.Article">
    select *from article order by a_id desc   limit #{pagenum},6  
    </select>

<select id="selectall" resultType="com.safecode.entity.Article">
        select *from article order by a_id desc
    </select>


    
</mapper>